enum Permission {
  ADMIN
  USER
}

type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  permissions: [Permission]
}

type Photo {
  id: ID! @unique
  title: String!
  photographer: String
  photographerLink: String
  description: String
  thumbnail: String!
  largeImage: String!
  price: Int!
  height: Int!
  width: Int!
  createdAt: DateTime!
  updatedAt: DateTime!
  user: User!
}