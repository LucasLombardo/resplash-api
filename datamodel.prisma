enum Permission {
  ADMIN
  USER
  PHOTOCREATE
  PHOTOUPDATE
  PHOTODELETE
  PERMISSIONUPDATE
}

type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  resetToken: String
  resetTokenExpiry: String
  permissions: [Permission]
}

type Photo {
  id: ID! @unique
  title:  String!
  photographer: String
  photographerLink: String
  description: String
  thumbnail: String!
  largeImage: String!
  price: Int!
  createdAt: DateTime!
  updatedAt: DateTime!
  user: User!
}